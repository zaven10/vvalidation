{
  "name": "vvalidation",
  "version": "1.0.0-20210828",
  "description": "Validation for vue 3 with vuelidate 2",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "npm run build && node tests/test.js",
    "build": "npm run clean && npx tsc",
    "clean": "shx rm -rf dist"
  },
  "keywords": [
    "vvalidation",
    "vuelidate 2 validation",
    "vue3 validation",
    "vuelidate",
    "v validation",
    "vue validation",
    "vuelidate class validator",
    "vuelidate decorators"
  ],
  "author": "Zaven",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "shx": "^0.2.2"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.24",
    "@vuelidate/validators": "^2.0.0-alpha.21",
    "typescript-bundle": "^1.0.17",
    "vue": "^3.2.6"
  }
}
